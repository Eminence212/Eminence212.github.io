  border: 0;
  padding: 12px 34px;
  color: #fff;
  transition: 0.4s;
  border-radius: 50px;
}
.contact .formulaire button[type=submit]:hover {
  background: #209dd8;
}

/*--------------------------------------------------------------
  # Footer
  --------------------------------------------------------------*/
